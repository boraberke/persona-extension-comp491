{
    "name": "Hansel",
    "description": "Fooling Web Trackers since 2022",
    "version": "1.0.0",
    "content_security_policy": {
        "extension_pages": "script-src 'self' ; object-src 'self'"
    },
    "action": {
        "default_popup": "index.html",
        "default_icon": "icons/Icon-192.png"
    },
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png" },
    "manifest_version": 3,
    "permissions": [
        "tabs"
      ]
}

